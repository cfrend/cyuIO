{
  "name": "cyuio",
  "version": "0.0.1",
  "description": "The beginnings of CYUTIL.io web info, store, and services.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && xcopy \"src/index.html\" \"dist/\" /F /Y && webpack-dev-server --content-base src/ --inline --hot --history-api-fallback",
